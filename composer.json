{
    "name": "jackweinbender/larvael-jsonapi",
    "description": "Convenience classes for sending and recieveing JSONAPI payloads",
    "keywords": [
        "php",
        "laravel",
        "eloquent",
        "json-api",
        "jsonapi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jack Weinbender",
            "email": "jack.weinbender@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "illuminate/database": "~5.0",
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.7"
    },
    "autoload": {
        "psr-4": {
            "Jackweinbender\\LaravelJsonapi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jackweinbender\\LaravelJsonapi\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
